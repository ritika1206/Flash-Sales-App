<%= notice %>

<%= form_with model: @user, method: :patch, url: "http://127.0.0.1:3000/users/:user_id/set-new-password/#{@user.id}"  do |form| %>
  <%= form.label :password %>
  <%= form.password_field :password %>

  <%= form.label :password_confirmation %>
  <%= form.password_field :password_confirmation %>

  <%= form.submit 'Submit' %>
<% end %>
